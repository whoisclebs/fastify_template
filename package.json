{
	"name": "fastify_template",
	"version": "0.0.1",
	"main": "src/main/index.ts",
	"scripts": {
		"build": "tsup",
		"lint": "biome check .",
		"lint:fix": "biome format . --write",
		"test": "jest --passWithNoTests --no-cache --runInBand",
		"test:coverage": "npm test -- --coverage",
		"test:watch": "npm test -- --watch",
		"test:staged": "npm test -- --findRelatedTests",
		"prepare": "husky install && chmod ug+x .husky/*",
		"commit:lint": "commitlint --edit",
		"start": "tsup src/main/index.ts --format cjs,esm --dts --out-dir dist --clean --watch --onSuccess \"node dist/index.js\""
	},
	"keywords": [
		"typescript",
		"fastify",
		"template"
	],
	"author": "whoisclebs",
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@commitlint/cli": "^17.1.2",
		"@commitlint/config-conventional": "^17.1.0",
		"@types/jest": "^29.5.14",
		"@types/node": "^18.19.86",
		"husky": "^8.0.1",
		"jest": "^29.1.2",
		"lint-staged": "^11.1.2",
		"ts-jest": "^29.0.3",
		"tsup": "^8.4.0",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"@fastify/cors": "^11.0.1",
		"@fastify/helmet": "^13.0.1",
		"@fastify/rate-limit": "^10.2.2",
		"dotenv": "^16.5.0",
		"fastify": "^5.3.2",
		"fastify-plugin": "^5.0.1",
		"pino": "^9.6.0",
		"ts-node": "^10.9.1"
	}
}
